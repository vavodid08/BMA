var minusculas=/[a-z]/;var mayusculas=/[A-Z]/;var digitos=/[0-9]/;var simbolos=/[!?@#$%*()_\-+]/;function variedad(a){var b=0;b+=minusculas.test(a)?1:0;b+=mayusculas.test(a)?1:0;b+=digitos.test(a)?1:0;b+=simbolos.test(a)?1:0;return b}function medirFortaleza(c){var a=c.length;var b=variedad(c);if(a==0){return 0}else{if(a<4){return 1}else{if(a<8||b<3){return 2}else{if(b==3){return 3}}}}return 4}function transitionTo(a,d){var c=250;var b=a.find(".leyenda.pos"+d);a.find(".leyenda:not(.pos"+d+")").stop().hide().promise().done(function(){b.fadeIn(c)});a.find(".led").stop().animate({"background-color":b.css("color"),width:(d*25)+"%"},c)};