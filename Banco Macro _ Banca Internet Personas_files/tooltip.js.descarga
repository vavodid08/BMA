var mouseOver=false;var timeoutVar;function showTooltip(a,b){mouseOver=true;timeoutVar=setTimeout(function(){show(a,b)},500)}function show(b,c){if(mouseOver==true){var d=b.id+"_div";var g=$("#"+b.id);var a=g.position();var f,e;if(c=="bottom"){f=0;e=a.top+40}else{f=a.left+g.width()+4;e=a.top}$("#"+d).css("left",f).css("top",e).fadeIn().css("display","table")}}function hideTooltip(a){var b=a.id+"_div";$("#"+b).fadeOut();mouseOver=false;clearTimeout(timeoutVar)};